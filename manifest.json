{
	"manifest_version": 3,
	"name": "Find sus link",
	"version": "1.0",
	"description": "Ad-blocker extension for Chrome",
	"permissions": [
		"activeTab",
		"tabs",
		"webRequest",
		"webRequestBlocking",
		"*://*/*"
	],
	"background": {
		"service_worker": ["background.js"]
	},
	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"js": ["content.js", "detectPopups.js"]
		}
	],
	"action": {
		"default_icon": {
		  "16": "images/icon16.png"
		}
	}
}