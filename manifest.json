
{
	"manifest_version": 2,
	"name": "Find sus link",
	"version": "1.0",
	"description": "Ad-blocker extension for Chrome",
	"permissions": [
		"activeTab"
	],
	"background": {
		"scripts": ["background.js"],
		"persistent": false
	},
	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"js": ["content.js"]
		}
	],
	"browser_action": {
		"default_popup": "popup.html",
		"default_icon": {
		  "16": "images/icon16.png"
		}
	}
}

