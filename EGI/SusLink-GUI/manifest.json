{
  "manifest_version": 3,
  "name":"FindSusLink",
  "version": "0.1",
  "description": "ad blocker and malicious webpage finder",
  "icons": {
    "16": "assets/susguardlogo-16.png",
    "32": "assets/susguardlogo-32.png",
    "48": "assets/susguardlogo-48.png",
    "128": "assets/susguardlogo-128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["content.js"]
    }
  ],
  "action": {
    "default_popup": "popup.html"
  }

}
